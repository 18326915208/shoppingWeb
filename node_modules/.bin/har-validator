#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../._har-validator@2.1.3@har-validator/lib/bin.js" "$@"
  ret=$?
else 
  node  "$basedir/../._har-validator@2.1.3@har-validator/lib/bin.js" "$@"
  ret=$?
fi
exit $ret
